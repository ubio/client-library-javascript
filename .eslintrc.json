{
    "extends": "ub",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2015
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "prefer-arrow-callback": "off",
        "object-shorthand": "off",
        "no-var": "off"
    }
}
